.loading-view-layer{
    position: fixed;
    top:0px;
    left:0px;
    width: 100%;
    height: 100%;
    background: #fff;
    z-index: 999;
    font-size: 16px;
}
.loading-img-icon{
    width: 100%;
    max-height: 100%;
    margin-bottom:5px;
}
.loading-view-body{
    position: absolute;
    width: 110px;
    height: 120px;
    left:0px;
    right: 0px;
    bottom: 0px;
    top:0px;
    margin:auto;
    text-align: center;
}